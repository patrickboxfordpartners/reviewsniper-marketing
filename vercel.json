{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval' 'unsafe-inline' https://cdn.auth0.com https://unpkg.com https://cdn.tailwindcss.com https://fonts.googleapis.com;"
        }
      ]
    }
  ]
}
